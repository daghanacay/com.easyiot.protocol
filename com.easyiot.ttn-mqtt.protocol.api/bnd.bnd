Bundle-Version:1.0.0.${tstamp}
Bundle-Description: 				\
	TTN-MQtt protocol implementation project defines DTOs for the ttn network

	
Export-Package: \
	com.easyiot.ttn_mqtt.protocol.api;provide:=true,\
	com.easyiot.ttn_mqtt.protocol.api.dto


Require-Capability: \
